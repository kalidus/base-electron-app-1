{
  "name": "basicapp_2",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack --watch",
    "build": "webpack --mode production",
    "electron-dev": "set NODE_ENV=development && electron .",
    "package": "electron-builder build --win --publish never",
    "start-dev": "concurrently \"npm run dev\" \"npm run electron-dev\""
  },
  "build": {
    "appId": "com.example.basicapp",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Aplicaci√≥n de escritorio multiplataforma con Electron y PrimeReact",
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "node-ssh": "^13.2.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ssh2": "^1.16.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-for-react": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
